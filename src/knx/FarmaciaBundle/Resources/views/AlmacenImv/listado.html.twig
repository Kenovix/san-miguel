{% extends "::simple.html.twig" %}

{% block stylesheets %}
{% endblock %}

{% block head %}
{% endblock %}


{% block body %}

        <p><b>Existencias en Almacen: {{ almacen }}</b><br/><br/><br/>


	      {% if almacenimv %}
	       <table border="1">

	       <thead>
	            <tr>
	                <th width="30px"><b>No.</b></th>
	                <th width="500px"><b>Nombre</b></th>
			<th width="50px"><b>Cant.</b></th>

	            </tr>
	        </thead>
	        <tbody>
	        {% for almacenimv in almacenimv %}
	            <tr>
	               <td width="30px">{{ loop.index }}</td>
		           <td width="500px">{{ almacenimv.imv }}</td>
	               <td width="50px">{{ almacenimv.cant }}</td>
	            </tr>

	        {% endfor %}
	        </tbody>
	    </table>
	    {% endif %}
{% endblock %}


{% block footer %}
{% endblock %}